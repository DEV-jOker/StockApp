<% if user_objects.count > 0 %>
  <table class="table table-striped table-hover" id='user-friends'>
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">Stocks</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
      <%= render user_objects %>
    </tbody>
  </table>
<% end %>
